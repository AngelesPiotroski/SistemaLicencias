<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 
    <class name="modelo.DiasTomados"  table="DiasTomados" schema="public" optimistic-lock="version">
     <id name="IdDiasTomados" column="ID_DIASTOMADOS"   type="long"> <generator class="native" /> </id>
    
    <property name="cantDiasTomados" column="CANTDIASTOMADOS" type="Integer">    </property>
    
    <many-to-one name="CorrespAnioTomado" 
        column="IdDiasCorrespondientesPorAnio"
        not-null="true"   cascade="save-update"/>

</hibernate-mapping>
